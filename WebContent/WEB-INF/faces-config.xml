<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">


  <navigation-rule>
    <from-view-id>/LoginCliente.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>cli</from-outcome>
      <to-view-id>/TelaInicial.xhtml?faces-redirect=true</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-outcome>vend</from-outcome>
      <to-view-id>/TelaPrincipalVendedor.xhtml</to-view-id>
    </navigation-case>
 
  </navigation-rule>
  
  <navigation-rule>
    <from-view-id>/TelaInicial.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>carrinho</from-outcome>
      <to-view-id>/CarrinhoCompras.xhtml</to-view-id>
    </navigation-case>
 	
 	    <navigation-case>
      <from-outcome>login</from-outcome>
      <to-view-id>/LoginCliente.xhtml</to-view-id>
    </navigation-case>
    
  </navigation-rule>
  
  <navigation-rule>
  
    <from-view-id>/TelaPrincipalVendedor.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>cadastro</from-outcome>
      <to-view-id>/CadastrarVendendor.xhtml</to-view-id>
    </navigation-case>
 	
 	   
    
  </navigation-rule>
  
  


</faces-config>
